<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>catfish</icon>
	<name>Find files</name>
	<unique-id>1450729426323825-1</unique-id>
	<command>catfish --fileman=bl-file-manager --hidden --path=%f</command>
	<description>Search for files with Catfish</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>utilities-terminal</icon>
	<name>Open terminal here</name>
	<unique-id>1450729426323924-2</unique-id>
	<command>sh -c &apos;cd %f;x-terminal-emulator&apos;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>gksu-root-terminal</icon>
	<name>Open root terminal here</name>
	<unique-id>1450729426324038-3</unique-id>
	<command>sh -c &apos;cd %f;gksudo x-terminal-emulator&apos;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Open as root</name>
	<unique-id>1450729426324118-4</unique-id>
	<command>gksudo &quot;bl-file-manager %f&quot;</command>
	<description>Open directory as root</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>accessories-text-editor</icon>
	<name>Open as root</name>
	<unique-id>1450729426324199-5</unique-id>
	<command>gksudo &quot;bl-text-editor %f&quot;</command>
	<description>Open text file as root</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>applications-system</icon>
	<name>Check md5 sum</name>
	<unique-id>1450729426324273-6</unique-id>
	<command>zenity --info --title=&quot;Check md5 for %n&quot; --text=&quot;$(md5sum %f)&quot;</command>
	<description></description>
	<patterns>*.iso;*.img;*.sub</patterns>
	<other-files/>
</action>
</actions>
